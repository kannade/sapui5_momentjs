<mvc:View controllerName="momentjs.nickcode.controller.Main" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<Text text="{path: 'nickcode>/time', formatter: 'momentjs.nickcode.model.formatter.requestTime'}" class="requestTime"/>
						</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>